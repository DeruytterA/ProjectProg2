<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>


<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" >
    <bottom>
        <HBox fx:id="hbox" alignment="CENTER" maxWidth="940.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="buttonBackAll" maxHeight="30.0" maxWidth="205.0" minHeight="30.0" minWidth="205.0" mnemonicParsing="false" onAction="#buttonBackAll" prefHeight="30.0" prefWidth="205.0" text="|&lt;">
                    <padding>
                        <Insets left="10.0" right="10.0" />
                    </padding>
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="buttonBack" maxHeight="30.0" maxWidth="205.0" minHeight="30.0" minWidth="205.0" mnemonicParsing="false" onAction="#buttonBack" prefHeight="30.0" prefWidth="250.0" text="&lt;">
                    <padding>
                        <Insets left="10.0" right="10.0" />
                    </padding>
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="buttonForward" maxHeight="30.0" maxWidth="205.0" minHeight="30.0" minWidth="205.0" mnemonicParsing="false" onAction="#buttonForward" prefHeight="30.0" prefWidth="250.0" text="&gt;">
                    <padding>
                        <Insets left="10.0" right="10.0" />
                    </padding>
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="buttonForwardAll" maxHeight="30.0" maxWidth="205.0" minHeight="30.0" minWidth="205.0" mnemonicParsing="false" onAction="#buttonForwardAll" prefHeight="30.0" prefWidth="250.0" text="&gt;|">
                    <padding>
                        <Insets left="10.0" right="10.0" />
                    </padding>
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </bottom>
</BorderPane>
