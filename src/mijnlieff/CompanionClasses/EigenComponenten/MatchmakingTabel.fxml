<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="vbox" alignment="CENTER" prefHeight="412.0" prefWidth="572.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ListView fx:id="spelersLijstView" prefHeight="389.0" prefWidth="572.0" />
        <HBox alignment="CENTER" prefHeight="31.0" prefWidth="572.0">
            <children>
                <Button fx:id="verwijderUitLijst" mnemonicParsing="false" onAction="#verwijderUitLijst" text="Stel jezelf niet meer kandidaat">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
                </Button>
                <Button fx:id="voegtoeAanlijst" mnemonicParsing="false" onAction="#voegToeAanlijst" text="Stel jezelf kandidaat">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
                </Button>
                <Button fx:id="kiesSpeler" mnemonicParsing="false" onAction="#kiesSpeler" text="Kies speler">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
                </Button>
            <Button fx:id="refreshList" mnemonicParsing="false" onAction="#doRefreshList" text="Refresh List">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            </children>
        </HBox>
    </children>
</fx:root>
