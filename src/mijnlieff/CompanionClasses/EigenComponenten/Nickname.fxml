<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import java.net.URL?>
<fx:root fx:id="vBox" alignment="CENTER" prefHeight="412.0" prefWidth="572.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@invalid.css"/>
    </stylesheets>
    <children>
      <Label text="Label">
          <VBox.margin>
              <Insets bottom="50.0" />
          </VBox.margin>
          <font>
              <Font size="31.0" />
          </font>
      </Label>
      <TextField fx:id="nicknameTextfield">
          <VBox.margin>
              <Insets left="250.0" right="250.0" />
          </VBox.margin>
      </TextField>
      <Button mnemonicParsing="false" text="Check Availability" fx:id="nickNameButton" onAction="#checkPressed" />
   </children>
</fx:root>
